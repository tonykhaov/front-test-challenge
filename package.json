    "@remix-run/react": "^2.5.0",
    "@remix-run/serve": "^2.5.0",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@pandacss/dev": "^0.27.2",
